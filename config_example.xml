<Config>
    <!-- The address which HTTP will bind to -->
    <address>127.0.0.1:8080</address>

    <!-- Credentials -->
    <username>username</username>
    <password>password</password>

    <!-- Database information-->
    <databaseAddress>127.0.0.1</databaseAddress>
    <databaseName>cmoc</databaseName>
    <assetsPath>mc-assets</assetsPath>

    <!-- OIDC credentials/data -->
    <oidc>
        <clientID>clientID</clientID>
        <clientSecret>clientSecret</clientSecret>
        <redirectURI>http://localhost:9011/auth</redirectURI>
        <endpoint>
            <authURL>https://accounts.google.com/o/oauth2/auth</authURL>
            <tokenURL>https://accounts.google.com/o/oauth2/token</tokenURL>
        </endpoint>
        <state>help-me</state>
    </oidc>
</Config>